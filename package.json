  "version": "0.1.0",
  "description": "UI to manage queries for graphql-query-whitelisting middleware",
  "main": "./dist/index.js",
  "scripts": {
    "lint": "eslint .",
    "test": "NODE_ENV=test NODE_PATH=src mocha --timeout 1000 --compilers js:babel-core/register --require babel-polyfill ./test/*.js",
